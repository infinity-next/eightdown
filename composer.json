{
    "name": "infinity-next/eightdown",
    "description": "Eightdown is a Markdown implementation for Laravel that adds optional features imageboard users will be familiar with.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Moon",
            "email": "josh@jaw.sh"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "InfinityNext\\Eightdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InfinityNext\\Tests\\Eightdown\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
